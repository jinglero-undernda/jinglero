{
  "name": "jinglero",
  "version": "0.1.0",
  "description": "La Usina de la Fabrica de Jingles - YouTube clips curation platform",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "migrate:pre-audit": "ts-node src/server/db/migration/pre-migration-audit.ts",
    "migrate:ids": "ts-node src/server/db/migration/migrate-entity-ids.ts",
    "migrate:ids:dry-run": "ts-node src/server/db/migration/migrate-entity-ids.ts --dry-run",
    "db:export-csv": "ts-node src/server/db/migration/export-to-csv.ts",
    "db:generate-id": "ts-node src/server/db/migration/generate-id.ts",
    "db:import-fabricas": "ts-node src/server/db/migration/import-fabricas.ts",
    "db:audit-relationships": "ts-node src/server/db/quality/audit-relationships-cli.ts",
    "db:fix-relationships": "ts-node src/server/db/quality/audit-relationships-cli.ts --fix"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "neo4j-driver": "^5.14.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.19.21",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
